%% For header
\def\Vol#1{\gdef\@Vol{#1}%
\gdef\@Volth{\@nth{#1}}%
 \ifx\@Vol\empty
  \gdef\@Year{\number\year}%
 \else
  \@tempcnta=#1\advance\@tempcnta 1986\relax
   \xdef\@Year{\the\@tempcnta}%
 \fi
}
\newcount\@tempcnta\@tempcnta=\year\advance\@tempcnta by -1986
\def\@Vol{\number\@tempcnta}
\def\@Volth{\@nth{22}}
%% BEGIN: original version in tugboat package: T.Kamishima 
%  Adapted from c.t.t posting by Donald Arseneau, 26 May 93.
\def\@nth#1{\newcount\@tempcnta\newcount\@tempcntb\relax
\@tempcnta=#1%
\@tempcntb=\@tempcnta%
\divide\@tempcntb by 100 \multiply \@tempcntb by 100%
\advance\@tempcnta by -\@tempcntb%
\ifnum\@tempcnta >20 \@tempcntb=\@tempcnta%
  \divide\@tempcntb by 10 \multiply \@tempcntb by 10%
  \advance\@tempcnta by -\@tempcntb%
\fi%
\number#1%
       \ifcase\@tempcnta th%    0th
       \or   st%                1st
       \or   nd%                2nd
       \or   rd%                3rd
       \else th%                nth
       \fi
}
%% END: original version in tugboat package: T.Kamishima
% END: modified by T.Kamishima 2008.3.7
